{
  "name": "@aka/core",
  "version": "0.1.2",
  "description": "Kalengo web framework helper",
  "main": "dist/src/index.js",
  "types": "src/index.ts",
  "scripts": {
    "lint": "tslint -p tsconfig.json -c tslint.json \"packages/**/*.ts\" -e \"*.spec.ts\" --fix",
    "clean": "gulp clean:bundle",
    "prebuild": "rm -rf node_modules/@akajs",
    "build": "gulp build --dist node_modules/@akajs",
    "postbuild": "gulp move",
    "prebuild:prod": "npm run clean",
    "build:prod": "gulp build",
    "integration-test": "mocha \"integration/*/{,!(node_modules)/**/}/*.spec.ts\" --reporter spec --require ts-node/register",
    "prerelease": "gulp copy-misc && gulp build --dist node_modules/@akajs",
    "publish": "npm run prerelease && npm run build:prod && ./node_modules/.bin/lerna publish --registry https://registry.npmjs.org",
    "publish:rc": "npm run prerelease && npm run build:prod && ./node_modules/.bin/lerna publish --registry https://registry.npmjs.org --npm-tag=rc",
    "publish:next": "npm run prerelease && npm run build:prod && ./node_modules/.bin/lerna publish --registry https://registry.npmjs.org --npm-tag=next --skip-git",
    "publish:beta": "npm run prerelease && npm run build:prod && ./node_modules/.bin/lerna publish --registry https://registry.npmjs.org --npm-tag=beta",
    "publish:test": "npm run prerelease && npm run build:prod && ./node_modules/.bin/lerna publish --registry https://registry.npmjs.org --npm-tag=test --skip-git",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kaolalicai/aka"
  },
  "keywords": [
    "klg"
  ],
  "author": "nick",
  "license": "MIT",
  "engines": {
    "node": ">= 8.9.0"
  },
  "dependencies": {
    "class-validator": "^0.10.1",
    "config": "^3.2.2",
    "glob": "^7.1.6",
    "inversify": "^5.0.1",
    "inversify-binding-decorators": "^4.0.0",
    "inversify-inject-decorators": "^3.1.0",
    "klg-date": "^1.1.5",
    "klg-logger": "^3.0.1",
    "klg-number": "^1.0.7",
    "koa-bodyparser": "^4.2.1",
    "koa-morgan": "^1.0.1",
    "koa-router": "^7.4.0",
    "koa-static": "^5.0.0",
    "lodash": "^4.17.15",
    "mongoose": "^5.7.5",
    "reflect-metadata": "^0.1.13",
    "typegoose": "^5.9.1"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/koa-bodyparser": "^4.3.0",
    "@types/lodash": "^4.14.139",
    "@types/mongoose": "^5.5.18",
    "@types/koa": "^2.0.50",
    "@types/koa-router": "^7.0.42",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.7.8",
    "@types/supertest": "^2.0.8",
    "chai": "^4.2.0",
    "delete-empty": "^3.0.0",
    "gulp": "^4.0.2",
    "gulp-clean": "^0.4.0",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-typescript": "^5.0.1",
    "koa": "^2.10.0",
    "lerna": "^3.17.0",
    "mocha": "^6.2.0",
    "supertest": "^4.0.2",
    "ts-node": "^8.4.1",
    "tslint": "^5.20.0",
    "tslint-config-klg": "^2.0.0",
    "typescript": "^3.6.3",
    "vuepress": "^1.2.0",
    "webstorm-disable-index": "^1.2.0"
  }
}
